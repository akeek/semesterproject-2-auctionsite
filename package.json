  "version": "0.1.2",
  "description": "auction-app",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "test": "node src/**/*.js",
    "lint": "eslint src/**/*.js",
    "lint-fix": "eslint src/**/*.js --cache --fix",
    "format": "prettier -w src/**/*.js",
    "build": "sass src/scss:dist/css",
