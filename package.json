    "url": "git+https://github.com/akeek/semesterproject-2-auctionsite.git"
  },
  "keywords": [
    "css",
    "js",
    "html",
    "api",
    "auction",
    "sneakers"
  ],
  "author": "Ã…ke Ek <ake.ek@live.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/akeek/semesterproject-2-auctionsite/issues"
  },
  "homepage": "https://github.com/akeek/semesterproject-2-auctionsite#readme",
  "devDependencies": {
    "eslint": "^8.29.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.4",
    "prettier": "^2.8.0",
    "sass": "^1.56.1"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.html": [
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write"
    ],
    "*.css": [
      "prettier --write"
